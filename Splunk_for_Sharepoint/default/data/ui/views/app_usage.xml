<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>Usage</label>
	<row>
		<table>
			<searchTemplate>eventtype="mssharepoint-iis"|stats count(cs_username) as "#Visits" by cs_username|rename cs_username as UserName</searchTemplate>
			<title>By Unique Users</title>
		</table>
	</row>
  
	<row>
		<chart>
			<searchTemplate>eventtype="mssharepoint-iis" cs_uri_stem!="\_*" | timechart count(cs_uri_stem)</searchTemplate>
			<title>Overtime by Page Impression</title>
			<option name="charting.axisTitleX.text">Time</option>
			<option name="charting.axisTitleY.text"># Page Impression</option>
			<option name="charting.chart">column</option>
			<option name="charting.chart.nullValueMode">gaps</option>
			<option name="charting.chart.stackMode">default</option>
			<option name="charting.layout.splitSeries">false</option>
			<option name="charting.legend.placement">right</option>
			<option name="charting.primaryAxisTitle.text"></option>
			<option name="charting.secondaryAxisTitle.text"></option>
		</chart>
	</row>
  
</dashboard>
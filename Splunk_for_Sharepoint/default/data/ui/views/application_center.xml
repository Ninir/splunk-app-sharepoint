<?xml version='1.0' encoding='utf-8'?>
<dashboard>
  <label>Application Center</label>
  <row>
    <table>
      <searchTemplate>sourcetype="SharepointAuditLogger" | top ApplicationName, UserName|Fields ApplicationName, UserName</searchTemplate>
      <title>Top Applications by Unique Users</title>
    </table>
  </row>
  <row>
    <chart>
      <searchTemplate> sourcetype="SharepointAuditLogger" Event="View" | top ApplicationName limit=50</searchTemplate>
      <title>Top Apps by Document Downloads</title>
      <option name="charting.chart">pie</option>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </chart>
    <chart>
      <searchTemplate>sourcetype="SharepointAuditLogger" Event="Update" | top ApplicationName limit=50</searchTemplate>
      <title>Top Apps by Document Uploads</title>
      <option name="charting.chart">pie</option>
      <option name="count">10</option>
      <option name="displayRowNumbers">true</option>
    </chart>
  </row>
  
</dashboard>

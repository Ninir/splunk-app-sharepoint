<dashboard>
 <label>Health Overview</label>
 <row>
  <single>
   <title>Farms</title>
   <searchName>Ops Overview - Farm Count</searchName>
   <option name="additionalClass">MSSP_Ops_Farm</option>
   <option name="afterLabel">Farm(s)</option>
  </single>
  <chart>
   <title>Page Impressions (Last 10 Minutes)</title>
   <searchName>Ops Overview - Page Impressions</searchName>
   <option name="charting.chart">radialGauge</option>
   <option name="charting.legend.placement">none</option>
   <option name="height">150px</option>
  </chart>
  <chart>
   <title>Unique Users (Last 10 Minutes)</title>
   <searchName>Ops Overview - Unique Users</searchName>
   <option name="charting.chart">radialGauge</option>
   <option name="charting.legend.placement">none</option>
   <option name="height">150px</option>
  </chart>
 </row>
 <row>
  <table>
   <title>Failed Services</title>
   <searchName>Ops Overview - Service Status</searchName>
   <option name="count">10</option>
   <option name="showPager">true</option>
   <option name="displayRowNumbers">false</option>
  </table>
  <table>
   <title>Servers with No Connections (Last 15 Minutes)</title>
   <searchName>Ops Overview - No Volume</searchName>
   <option name="count">10</option>
   <option name="showPager">true</option>
   <option name="displayRowNumbers">false</option>
  </table>
  <table>
   <title>Servers with High Load (Last 15 Minutes)</title>
   <searchName>Ops Overview - High Load</searchName>
   <option name="count">10</option>
   <option name="showPager">true</option>
   <option name="displayRowNumbers">false</option>
  </table>
 </row>
</dashboard>

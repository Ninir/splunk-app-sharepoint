[sp13audit://<name>]
interval = <value>
* Polling interval for Audit Logs

dbcheckinterval = <value>
* Polling interval to check for new SharePoint content databases

autoenableinterval = <value>
* Polling interval to check for new SharePoint sites to auto-enable auditing

autoenable = <value>
* Determine if Sharepoint Audit is enabled by the Modular Input
